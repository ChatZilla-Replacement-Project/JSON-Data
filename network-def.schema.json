{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"name": {
				"type": "string"
			},
			"homepage": {
				"type": "string",
				"format": "iri"
			},
			"servers": {
				"type": "array",
				"items": {
					"type": "string",
					"format": "iri"
				}
			},
			"module": {
				"type": "string",
				"pattern": "^[\\d-_a-z]+$"
			}
		}
	}
}
